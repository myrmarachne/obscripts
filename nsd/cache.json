{
  "name":"webserver + cache",
  "vendor":"FOKUS",
  "version":"1.0",
  "vld":[
    {
      "name":"private"
    }
  ],
  "vnfd":[
    {
      "name":"cache",
      "vendor":"FOKUS",
      "version":"1.0",
      "lifecycle_event":[
        {
          "event":"CONFIGURE",
          "lifecycle_events":[
            "server_configure_floatingIp.sh",
            "vnf_cache/configure.sh"
          ]
        },
        {
          "event":"INSTANTIATE",
          "lifecycle_events":[
            "vnf_cache/install.sh"
          ]
        }
      ],
      "vdu":[
        {
          "vm_image":[
            "ubuntu-14.04-server-cloudimg-amd64-disk1"
          ],
          "scale_in_out":1,
          "vnfc":[
            {
              "connection_point":[
                {
                  "floatingIp":"random",
                  "virtual_link_reference":"private",
                  "interfaceId":0
                }
              ]
            }
          ],
          "vimInstanceName":[]
        }
      ],
      "virtual_link":[
        {
          "name":"private"
        }
      ],
      "deployment_flavour":[
        {
          "flavour_key":"m1.samsung"
        }
      ],
      "type":"client",
      "endpoint":"generic",
      "vnfPackageLocation":"https://github.com/myrmarachne/obscripts.git"
    },
    {
      "name":"webserver",
      "vendor":"FOKUS",
      "version":"1.0",
      "lifecycle_event":[
        {
          "event":"INSTANTIATE",
          "lifecycle_events":[
            "webserver/install.sh"
          ]
        }
      ],
      "virtual_link":[
        {
          "name":"private"
        }
      ],
      "deployment_flavour":[
        {
          "flavour_key":"m1.samsung"
        }
      ],
      "vdu":[
        {
          "vm_image":[
            "ubuntu-14.04-server-cloudimg-amd64-disk1"
          ],
          "scale_in_out":1,
          "vnfc":[
            {
              "connection_point":[
                {
                  "floatingIp":"random",
                  "virtual_link_reference":"private",
                  "interfaceId":0
                }
              ]
            }
          ],
          "vimInstanceName":[]
        }
      ],
      "type":"server",
      "endpoint":"generic",
      "vnfPackageLocation":"https://github.com/myrmarachne/obscripts.git"
    }
  ],
   "vnf_dependency":[
    {
      "source":{
        "name":"webserver"
      },
      "target":{
        "name":"cache"
      },
      "parameters":[
        "private_floatingIp"
      ]
    }
  ]
}
